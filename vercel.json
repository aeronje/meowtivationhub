{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },

    {
      "src": "/analytics",
      "dest": "/analytics.html"
    },
    {
      "src": "/analyticsraw",
      "dest": "/analyticsraw.html"
    },

    {
      "src": "/",
      "dest": "/index.html"
    },

    {
      "src": "/(.*)",
      "dest": "/$1.html"
    }
  ]
}
